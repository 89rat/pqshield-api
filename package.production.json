{
  "name": "pq359",
  "version": "1.0.0",
  "description": "The world's first gamified quantum-resistant security platform",
  "type": "module",
  "main": "dist/index.js",
  "homepage": "https://pq359.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/89rat/pqshield-api.git"
  },
  "bugs": {
    "url": "https://github.com/89rat/pqshield-api/issues"
  },
  "keywords": [
    "quantum-resistant",
    "cybersecurity",
    "gamification",
    "neural-networks",
    "family-protection",
    "viral-growth"
  ],
  "author": "PQ359 Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:production": "vitest run tests/production.test.js",
    "test:security": "npm audit && snyk test",
    "test:load": "artillery run tests/load-test.yml",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "docker:build": "docker build -f Dockerfile.production -t pq359:latest .",
    "docker:run": "docker run -p 3000:3000 pq359:latest",
    "docker:compose": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose -f docker-compose.production.yml down",
    "deploy": "./scripts/deploy-production.sh",
    "deploy:staging": "./scripts/deploy-production.sh staging",
    "deploy:cloudflare": "wrangler deploy --env production",
    "deploy:firebase": "firebase deploy --project $FIREBASE_PROJECT_ID",
    "health-check": "curl -f https://pq359.com/health",
    "monitor": "node scripts/monitor-production.js",
    "backup": "node scripts/backup-production.js",
    "rollback": "node scripts/rollback-production.js",
    "security:scan": "npm audit && snyk test && semgrep --config=auto .",
    "performance:audit": "lighthouse https://pq359.com --output json --output-path ./reports/lighthouse.json",
    "analytics:report": "node scripts/generate-analytics-report.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:seed": "node scripts/seed-database.js",
    "cache:clear": "node scripts/clear-cache.js",
    "logs:tail": "wrangler tail --env production",
    "logs:download": "node scripts/download-logs.js",
    "maintenance:on": "node scripts/maintenance-mode.js on",
    "maintenance:off": "node scripts/maintenance-mode.js off",
    "ssl:renew": "node scripts/renew-ssl.js",
    "cdn:purge": "node scripts/purge-cdn.js",
    "neural:retrain": "node scripts/retrain-neural-networks.js",
    "viral:optimize": "node scripts/optimize-viral-mechanics.js",
    "compliance:check": "node scripts/compliance-check.js",
    "mobile:build": "cd pq359_mobile_app && flutter build apk --release",
    "mobile:test": "cd pq359_mobile_app && flutter test",
    "mobile:deploy": "cd pq359_mobile_app && fastlane deploy",
    "postinstall": "patch-package && husky install",
    "prepack": "npm run build",
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@firebase/app": "^0.9.25",
    "@firebase/auth": "^1.5.1",
    "@firebase/firestore": "^4.4.0",
    "@firebase/functions": "^0.11.0",
    "@firebase/storage": "^0.12.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-button": "^0.1.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/stripe-js": "^2.4.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "stripe": "^14.9.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "artillery": "^2.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "semgrep": "^1.52.0",
    "snyk": "^1.1266.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.4",
    "wrangler": "^3.22.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test && npm run security:scan"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "semver": "^7.5.4"
    }
  }
}
